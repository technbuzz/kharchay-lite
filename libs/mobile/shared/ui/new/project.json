{
  "name": "mobile-shared-ui-new",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/mobile/shared/ui/new/src",
  "prefix": "kh",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/mobile/shared/ui/new"],
      "options": {
        "jestConfig": "libs/mobile/shared/ui/new/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/mobile/shared/ui/new/src/**/*.ts",
          "libs/mobile/shared/ui/new/src/**/*.html"
        ]
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/angular",
        "port": 4400,
        "projectBuildConfig": "mobile-shared-ui-new:build-storybook",
        "configDir": "libs/mobile/shared/ui/new/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputDir}"],
      "options": {
        "uiFramework": "@storybook/angular",
        "projectBuildConfig": "mobile-shared-ui-new:build-storybook",
        "configDir": "libs/mobile/shared/ui/new/.storybook",
        "outputDir": "dist/storybook/mobile-shared-ui-new"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": ["type-ui"]
}
