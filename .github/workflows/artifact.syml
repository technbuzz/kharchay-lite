name: Artifact

on:  [push]

env:
  ARTIFACT_NAME: kharchay

jobs:
  main:
    runs-on: ubuntu-latest
    steps:
    - name: Checkout Code
      uses: actions/checkout@v3
    - name: Upload artifact
      uses: actions/upload-artifact@v3
      with:
        name: ${{ env.ARTIFACT_NAME }}
        path: ./apps/kharchay
