{
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/kharchay/src",
  "prefix": "kh",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/kharchay",
        "index": "apps/kharchay/src/index.html",
        "main": "apps/kharchay/src/main.ts",
        "polyfills": "apps/kharchay/src/polyfills.ts",
        "tsConfig": "apps/kharchay/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/kharchay/src/favicon.ico",
          "apps/kharchay/src/assets",
          {
            "glob": "**/*.svg",
            "input": "node_modules/ionicons/dist/ionicons/svg",
            "output": "./svg"
          }
        ],
        "styles": [
          "apps/kharchay/src/global.scss",
          "apps/kharchay/src/theme/variables.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/kharchay/src/environments/environment.ts",
              "with": "apps/kharchay/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "kharchay:build:production"
        },
        "development": {
          "browserTarget": "kharchay:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "kharchay:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/kharchay/src/**/*.ts",
          "apps/kharchay/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/kharchay"],
      "options": {
        "jestConfig": "apps/kharchay/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
